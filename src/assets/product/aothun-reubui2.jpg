            $(document).on('click','[rel-script="suggestion-picker"]',function (e) {
                e.preventDefault();
                const _this = $(this);
                const text = _this.attr('data-name');
                const _control = $('.header-search__control.one-whole');
                _control.val(text);
                _control.focus();
                showSuggestions();
                suggestionsList.hide();
                spotlight_content.removeClass('is-active');
                handleSearch(text);
            });

            function handleSearch(keyword) {
                const API_URL = `${window.location.origin}/load-product?home_search=1&limit=4&page=1&sort=created_at&type=spotlight-header&keyword=${keyword}`;
                let search_resource = '<div class="text--center"><i>Không tìm thấy kết quả phù hợp!</i></div>'

                spotlight_wrapper.parent().addClass('is-active')
                spotlight_wrapper.html(loading);

                if ($(spotlight_search_close).length > 0) {
                    $(spotlight_search_close).addClass('is-active');
                }

                $.ajax({
                    url: API_URL,
                    type: "GET",
                    cache: false,
                }).done(function(res) {
                    if (res.success) {
                        search_resource = res.html ? res.html : search_resource;
                        spotlight_wrapper.html(search_resource);
                        spotlight_wrapper.addClass('is-active');
                        if(res.html != '') {
                            spotlight_wrapper.siblings('.view-more').show();
                            spotlight_wrapper.siblings('.view-more').attr('href', '/spotlight?keyword=' + keyword);
