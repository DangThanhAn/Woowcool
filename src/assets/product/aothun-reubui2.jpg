            function headerSearch() {

                let targetSearch;
                let last_keyword;
                let search_time_out;

                $(document).on('click', '[rel-script="header-search"]', function(e) {
                    e.preventDefault();
                    header_search.addClass('is-active');
                    $(spotlight_search_content).addClass('is-active');
                    targetSearch = e.target;
                    $('.nav__sub').siblings().removeClass('is-active');
                    $('.site-header').css('z-index', '15');
                    $('.spotlight-header-search__viewmore').removeClass('is-active');
                    if($('.recent-product-list').length == 0) getRecentViewed()
                    setTimeout(function(e) {
                        search_input.focus();
                    }, 500)
                });

                $(document).on('click', function(e) {
                    // const _target = $(e.target);
                    // if (!targetSearch) return;
                    // if (!_target.parents('[rel-script="header-search"]').hasClass('is-active') && e.target != targetSearch) {
                    //     header_search.removeClass('is-active');
                    // }
                });


                $(document).on('click', spotlight_search_close, function(e) {
                    e.preventDefault();
                    search_input.focus();
                    removeActiveHeader();
                    $(this).removeClass('is-active');
                    resetSearchHeader()
                })

                search_input.on('keyup', function(e) {
                    const _this = $(this);
                    const value = _this.val();
                    if (value.trim() === "") {
                        resetSearchHeader()
                        return;
                    }
                    if (!value) {
                        resetSearchHeader()
                        return;
                    }

                    if (value === last_keyword) {
                        resetSearchHeader()
                        return;
                    }
                    if (search_time_out !== undefined) clearTimeout(search_time_out);

                    showSuggestions();

                    search_time_out = setTimeout(function(e) {
                        spotlight_content.removeClass('is-active');
                        handleSearch(value);
                    }, 500)


                })
            }

            function headerScroll() {
                let last_scroll_point = $(window).scrollTop();

                if (last_scroll_point > 0) {
                    header.addClass('is-scroll-top');
                }

                $(window).on('scroll', function(e) {
                    const scroll_point = $(this).scrollTop();
                    if(!$(header_search).hasClass('is-active')){
                        if (scroll_point > last_scroll_point && scroll_point > 100) {
                            header.addClass('is-scroll-top');

                        } else {
                            if (header.hasClass('is-scroll-top')) {
                                header.removeClass('is-scroll-top');

                            }
                        }
                    }
                    last_scroll_point = scroll_point;
                });
            }

