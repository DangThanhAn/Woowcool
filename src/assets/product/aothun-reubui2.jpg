            function showSuggestions() {
                // Clear the suggestions list
                suggestionsList.empty();

                suggestionsList.show();
                let userInput = inputSpotlight.val().toLowerCase().trim();
                let filteredSuggestions = suggestions[userInput] || [];

                if(filteredSuggestions.length >0) {
                    filteredSuggestions.forEach(function(suggestion) {
                        let boldedSuggestion = suggestion.replace(new RegExp(userInput, "gi"), `<strong>$&</strong>`);
                        let li = $(`<li data-name="${suggestion}" rel-script="suggestion-picker">`).html(boldedSuggestion);
                        suggestionsList.append(li);
                    });
                }
