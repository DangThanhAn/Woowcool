        function playImageVideo() {
                const _videoToggle = $('[rel-script="video-normal-toggle"]');
                const _videoThumbnail = $('[rel-script="product-video-normal"]');
                const _video = document.getElementById('video-play');
                if(_videoToggle && _videoThumbnail &&_video ) {
                    if($('.product-single__video').hasClass('is-active')){
                        _video.pause()
                        _videoToggle.removeClass('is-active');
                        _videoThumbnail.removeClass('is-active');
                    }

                    //delay product video playing
                    const timeout = 500;
                    setTimeout(() => {
                        _video.play();
                    }, timeout);

                    _videoToggle.on('click',function(e){
                        e.preventDefault();
                        const _this = $(this);
                        _this.toggleClass('is-active');
                        _videoThumbnail.toggleClass('is-active');
                        _video.load();
                        _video.play();
                    });

                    onChangeImageVideoSlick()

                    $(document).on('click','[rel-script="product-thumbs-slide"] .image',function(e){
                        _videoToggle.removeClass('is-active');
                        _videoThumbnail.removeClass('is-active');
                    });
                }
        }

        function checkCanPlayImageVideo(_videoToggle, _videoThumbnail, _video) {
            if(_videoToggle && _videoThumbnail &&_video ) {

                _video.pause()
                _videoToggle.removeClass('is-active');
                _videoThumbnail.removeClass('is-active');

                //play video if slide of image video is active
                if($("[data-slick-index=0]").hasClass('slick-active')) {
                        _videoToggle.toggleClass('is-active');
                        _videoThumbnail.toggleClass('is-active');
                        _video.load();
                        _video.play();
                }
            }
        }

        function onChangeImageVideoSlick() {
            const _videoToggle = $('[rel-script="video-normal-toggle"]');
            const _videoThumbnail = $('[rel-script="product-video-normal"]');
            const _video = document.getElementById('video-play');
            if(_videoToggle && _videoThumbnail &&_video ) {
                $('.slick-slider').keydown(function(e) {
                    let keyLeftCode = 37
                    let keyRightCode = 39
                    if(e.keyCode === keyLeftCode || e.keyCode === keyRightCode)
                        checkCanPlayImageVideo(_videoToggle, _videoThumbnail, _video)
                });

                $(document).on('swipe', '.slick-slider', function(event) {
                    checkCanPlayImageVideo(_videoToggle, _videoThumbnail, _video)
                })

                $('.slick-arrow').on('click', function(event, slick, direction) {
                    checkCanPlayImageVideo(_videoToggle, _videoThumbnail, _video)
                })

                $('.slick-dots').on('click', function(event, slick, direction) {
                    checkCanPlayImageVideo(_videoToggle, _videoThumbnail, _video)
                })
            }
        }

        function productQuantityChange() {
            $(document).on('click', '[rel-script="quantity-change"] .quantity__augure', function (e) {
                e.preventDefault();
                const _this = $(this);
                const input = $('[rel-script="quantity-change"] input');
                const max = input.attr('max');
                const value = input.val();

                if (Number(max) > Number(value)) {
                    let quantity = Number(value) + 1;
                    input.val(quantity);
                } else {
                    if (Number(max) < 0) input.val(0);
                    else input.val(max);
                }
            });
            $(document).on('click', '[rel-script="quantity-change"] .quantity__reduce', function (e) {
                e.preventDefault();
                const _this = $(this);
                const input = $('[rel-script="quantity-change"] input');
                const min = input.attr('min');
                const max = input.attr('max');
                const value = input.val();


                if (Number(min) < Number(value)) {
                    let quantity = Number(value) - 1;
                    input.val(quantity);

                } else {
                    if (Number(max) <= Number(min)) {
                        input.val(0);
                    } else {
                        input.val(min);
                    }
                }
            });
        }

