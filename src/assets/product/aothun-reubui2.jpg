        const GetImageData = function (url, size) {
            if (!url) return
            let src
            const ext = url.split('.').pop()
            const file = url.split('.').slice(0, -1).join('.')
            src = `${file}.${ext}`;
            src = src.replace('image/', 'uploads/');

            if(size == 'full'){
                return window.cool_mate_settings.MEDIA_CDN_URL + '/cdn-cgi/image/quality=100' + src
            }else{
                return window.cool_mate_settings.MEDIA_CDN_URL + '/cdn-cgi/image/width=300,height=442,quality=100' + src
            }
        }

        function createElementImage(obj, size, type = '') {
            const {
                src,
                title,
                index
            } = obj;

            const image_url = GetImageData(src, size);

            let html_image = `<a href="#" class="image" rel-script="product-lightbox-gallery" data-image="${image_url}" data-index="${index}">
                <img src="${image_url}" alt="${title}" />
            </a>`;
            if (type === 'thumbs') html_image = `<div class="image" data-image="${image_url}" data-index="${index}">
                <img src="${image_url}" alt="${title}" />
            </div>`;


            return html_image;
        }

